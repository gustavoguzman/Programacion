#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{int i, j;
 for(i=1; i<=10; i++)
   for (j=1; j<=10; j++)
     cout<<"la suma es: "<<i<<" + "<<j<<" igual = "<<i+j<<endl;
     
   
getch();	
}
